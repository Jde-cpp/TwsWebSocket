{
	"$types":
	{
		"name":{ "length": 255 },
		"$target":{ "length": 255 },
		"description":{ "length": 2047 },
		"path":{ "length": 4095 },
		"bigId": { "type": "uint64", "sequence": true },
		"id": "uint32",
		"smallId": "uint16",
		"tinyId": "uint8",
		"day": "uint16",
		"cik": "uint32",
		"crc": "uint32",
		"cusip":{ "type":"char", "length":8 },
		"created": { "type": "dateTime", "default":"$now", "insertable":false, "updateable":false },
		"updated": { "type": "dateTime", "default":"$now", "insertable":false, "updateable":false }
	},
	"$smallValues":
	{
		"id": "smallId",
		"name": "name",
		"$surrogateKey":["id"],
		"$naturalKey":["name"]
	},
	"$values":
	{
		"id": "id",
		"name": "name",
		"$surrogateKey":["id"],
		"$naturalKey":["name"]
	},
	"$data":
	{
		"id":{ "sequence": true },
		"name": "name",
		"$surrogateKey":["id"],
		"$naturalKey":["name"]
	},
	"$nk":
	{
		"$parent": "$data",
		"attributes": "uint16?",
		"created": "created",
		"updated": { "type": "dateTime?", "insertable": false, "updateable": false },
		"deleted": { "type": "dateTime?", "insertable": false, "updateable": false }
	},
	"$description":
	{
		"$parent": "$nk",
		"target": "$target",
		"description": "description?",
		"$naturalKey": ["target"]
	},
	"$scripts": ["sql/twt_user_block.sql", "sql/twt_tag_ignore.sql", "sql/twt_user_insert.sql"],
	"dgrClasses":
	{
		"id": "crc",
		"name": { "length":150 },
		"$data": ["None", "COM", "EMF", "MF", "CALL", "PUT", "WT", "OA", "MIUS", "PFD", "IFUS"],
		"$surrogateKey":["id"],
		"$naturalKey":["name"]
	},
	"dgrFilings":
	{
		"id": {"type":"id", "sequence": true },
		"cik": "mrkCompanies",
		"formId": "dgrForms",
		"submit_cik": "cik?",
		"year": "uint8",
		"sequence": "uint32",
		"filingDay": "day",
		"$surrogateKey":["id"],
		"$naturalKey":["cik", "year", "sequence", "submit_cik"]
	},
	"dgrForms":
	{
		"$parent": "$smallValues",
		"$data": ["None", "10-K", "2-E", "3", "4", "5", "6-K", "7-M", "8-K", "9-M", "10-Q", "11-K", "12b-25", "13F-HR", {"id":114, "name":"114"} ]
	},
	"ibAccounts":
	{
		"$parent": "$description"
	},
	"ibAccountRoles":
	{
		"roleId": "umRoles",
		"accountId": "ibAccounts",
		"rightId": { "type": "umRights", "default":"4" },
		"$surrogateKey":["roleId","accountId"]
	},
	"ibStockContracts":
	{
		"id": "uint32",
		"cusip": "cusip",
		"primary_exchange_id": "tinyId",
		"name": { "length": 255 },
		"symbol": { "length": 10 },
		"$surrogateKey":["id"]
	},
	"mrkCusips":
	{
		"id": "cusip",
		"name": "name",
		"cik": "cik?",
		"$surrogateKey":["id"]
	},
	"mrkCompanies":
	{
		"id": "cik",
		"name": "name",
		"$surrogateKey":["id"]
	},
	"twtBlocks":
	{
		"user_id": "uint64",
		"created": "created",
		"$surrogateKey":["user_id"]
	},
	"twtTags":
	{
		"id":{ "sequence": true },
		"tag": { "length": 255 },
		"ignored_count": "uint32",
		"$surrogateKey":["id"],
		"$naturalKey":["tag"]
	},
	"twtQueries":
	{
		"tag": { "length": 255 },
		"query": { "length": 512 },
		"$surrogateKey":["tag"]
	},
	"twtHandles":
	{
		"id": "uint64",
		"screen_name":  { "length": 20 },
		"blocked": "bool",
		"profile_image": { "type":"varchar?", "length": 512 },
		"updated": "updated",
		"$surrogateKey":["id"],
		"$naturalKey":["screen_name"]
	},
	"umUsers":
	{
		"$parent": "$description",
		"authenticatorId": "umAuthenticators?"
	},
	"umAuthenticators":
	{
		"$parent": "$values",
		"$data": ["None", "Google"]
	},
	"umGroups":
	{
		"$parent": "$description",
		"$data": [{"id":1, "attributes":5, "name":"Everyone", "target":"everyone" }, {"id":2, "attributes":6, "name":"Users", "target":"users"}]
	},
	"umUserGroups":
	{
		"userId": "umUsers",
		"groupId": "umGroups",
		"$surrogateKey":["userId", "groupId"]
	},
	"umRoles":
	{
		"$parent": "$description",
		"$data": [{"id":1, "attributes":4, "name":"User Management", "target": "user_management" }]
	},
	"umGroupRoles":
	{
		"groupId": "umGroups",
		"roleId": "umRoles",
		"$surrogateKey":["groupId","roleId"],
		"$data": [{"groupId":1, "roleId":1 }]
	},
	"umRights":
	{
		"$parent": "$values",
		"$flagsData": ["None","Administer", "Write", "Read"]
	},
	"umRolePermissions":
	{
		"roleId": "umRoles",
		"permissionId": "umPermissions",
		"rightId": { "type": "umRights", "default":"4" },
		"$surrogateKey":["roleId","permissionId"],
		"$data": [{"roleId":1, "permissionId":1, "rightId": 7}]
	},
	"umApis":
	{
		"$parent": "$values",
		"$data": ["None","UM", "Web", "Tws", "Blockly"]
	},
	"umPermissions":
	{
		"id":{ "sequence": true },
		"apiId": "umApis",
		"name": "name?",
		"$surrogateKey":["id"],
		"$naturalKey":["apiId","name"],
		"$data": [{"id":1, "apiId":1}, {"id":2, "apiId":3} ]
	}
}